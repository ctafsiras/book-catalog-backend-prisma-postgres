{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.ts",
        "methods": ["GET", "HEAD", "PUT", "PATCH", "POST", "DELETE", "OPTIONS"]
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  